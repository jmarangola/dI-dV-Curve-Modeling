
j = @(w, t) (((1)./(w.^3 + w)).* (((exp(-1.*1i.*w.*t)) - 1)./(1 - exp(-1.*w))));
p = @(E, t) (
kb=1.381e-23;

%bounds
tbounds = 0.001:dx:2000
wbounds = 0.001:dx:200;

dx = .1;

% loop vars
sum = 0;
sum2 = 0;

%iteration vars: matlab is 1 indexed not zero
k = 1;
j = 1;



 for t = tbounds
        for w=wbounds
            sum = sum + (j(w, t) * dx);
        end
        k = k + 1
        sum2 = (sum + exp(0 + 1i*E*t))*dx; % sum P(E) for a single value of int(e)_-inf i
        disp(sum)
 end
 


%figure
%plot(0.001:dx:2000, levels)


    

